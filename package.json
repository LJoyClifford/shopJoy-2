{
  "name": "shopjoy",
  "version": "1.0.0",
  "description": "eCommerce application built with the MERN stack",
  "type": "module",
  "main": "server.js",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "colors": "^1.4.0",
    "express": "^4.19.2",
    "js-tokens": "^4.0.0",
    "loose-envify": "^1.4.0",
